                                <div class="col-xs-12">
                                    @*<th class="column-title" style="display: table-cell;">Total: </th>
                                        <th class="column-title" style="display: table-cell;">$31.790 </th>*@

                                    <a class="btn btn-success pull-right" href="~/PAGO/Index">Pagar</a>
