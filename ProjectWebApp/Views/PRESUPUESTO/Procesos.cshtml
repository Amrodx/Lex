                                    @if (Model != null && ViewBag.Documento != null)
                                    {
                                        <embed src="data:application/pdf;base64,@System.Convert.ToBase64String(((ProjectWebApp.Models.DOCUMENTOS)ViewBag.Documento).DOCUMENTO_DIGITAL)" type="application/pdf" width="100%" height="100%" data="@((ProjectWebApp.Models.DOCUMENTOS)ViewBag.Documento).NOMBRE_ARCHIVO" />
                                    }
                                </div>
                                <div class="col-md-4 col-sm-12 col-xs-12 form-group">

                                </div>
                            </div>
                            <div class="row" style="text-align:center;">
                                <div class="checkbox-inline">
                                    <label class="hover">
                                        <div class="icheckbox_flat-green checked hover" style="position: relative;"><input type="checkbox" class="flat" checked="checked" style="position: absolute; opacity: 0;"><ins class="iCheck-helper" style="position: absolute; top: 0%; left: 0%; display: block; width: 100%; height: 100%; margin: 0px; padding: 0px; background: rgb(255, 255, 255); border: 0px; opacity: 0;"></ins></div> Acepto Contrato de Costos Basicos Generados por Presupuestos
                                    </label>
                                </div><br />
                                <div class="checkbox-inline">
                                    <label class="hover">
                                        <div class="icheckbox_flat-green checked hover" style="position: relative;"><input type="checkbox" class="flat" checked="checked" style="position: absolute; opacity: 0;"><ins class="iCheck-helper" style="position: absolute; top: 0%; left: 0%; display: block; width: 100%; height: 100%; margin: 0px; padding: 0px; background: rgb(255, 255, 255); border: 0px; opacity: 0;"></ins></div> Acepto Contrato de Gestion de Documentos Personales
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div id="step-4">
                            <h2 class="StepTitle">Step 4 Content</h2>
                            <p>
                                Status de Procesos Judiciales
                            </p>
                        </div>
